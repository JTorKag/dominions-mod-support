    "\u00ef\u00bb\u00bfFriendly name": "okundeadleader",
    "prefix": "#okundeadleader",
    "body": "~#okundeadleader~",
    "description": "This monster has an innate ability to command 40 undead beings. This is a rare ability that usually only demons or undead beings possess."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "older",
    "prefix": "#older",
    "body": "~#older~",
    "description": "Makes the monster start somewhat older or younger than its calculated or preset start age."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onebattlespell",
    "prefix": "#onebattlespell",
    "body": "~#onebattlespell~",
    "description": "Monster will automatically cast this spell just before the battle starts. There is no fatigue cost. This spell cannot be a spell added by a mod it must be an original Dominions 5 spell. You can modify the spell used by this command though."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onisummon",
    "prefix": "#onisummon",
    "body": "~#onisummon~",
    "description": "This unit has a chance of attracting one or more Ko-Oni per month. The Oni will only come if there is unrest or a turmoil scale in the province. The higher the unrest and turmoil the greater chance that a group of Ko-Oni will appear. The value of this ability will also modify this chance. A normal value is 50."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlyatsite",
    "prefix": "#onlyatsite",
    "body": "~#onlyatsite~",
    "description": "Ritual can only be cast in provinces containing this site."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlycoastsrc",
    "prefix": "#onlycoastsrc",
    "body": "~#onlycoastsrc~",
    "description": "1 means this ritual can only be cast from a coast province."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlycoldblood",
    "prefix": "#onlycoldblood",
    "body": "~#onlycoldblood~",
    "description": "The item can only be used by coldblooded beings."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlydemon",
    "prefix": "#onlydemon",
    "body": "~#onlydemon~",
    "description": "The item can only be used by demons."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlyfriendlydst",
    "prefix": "#onlyfriendlydst",
    "body": "~#onlyfriendlydst~",
    "description": "1 = can only target allied provinces 0 = turn this feature off (default)"
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlygeodst",
    "prefix": "#onlygeodst",
    "body": "~#onlygeodst~",
    "description": "Ranged ritual can only target one of these terrains. See table Terrain masks. Only used for spells that target faraway provinces."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlygeosrc",
    "prefix": "#onlygeosrc",
    "body": "~#onlygeosrc~",
    "description": "Ritual can only be cast from one of these terrains. See table Terrain masks for possible terrain mask values."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlyinanim",
    "prefix": "#onlyinanim",
    "body": "~#onlyinanim~",
    "description": "The item can only be used by inanimate beings."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlymnr",
    "prefix": "#onlymnr",
    "body": "~#onlymnr~",
    "description": "Only this type of monster is able to cast this ritual. Monster nbr can be negative for a montag. This command can be used multiple times on the same spell for non-montags."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlymounted",
    "prefix": "#onlymounted",
    "body": "~#onlymounted~",
    "description": "The item can only be used by mounted beings."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlyowndst",
    "prefix": "#onlyowndst",
    "body": "~#onlyowndst~",
    "description": "1 = can only target own provinces 0 = can target any province (default)"
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "onlyundead",
    "prefix": "#onlyundead",
    "body": "~#onlyundead~",
    "description": "The item can only be used by undead."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "order",
    "prefix": "#order",
    "body": "~#order~",
    "description": "Set special event orders that are enabled for the province (investigate, accept, decline etc). If several orders are needed to be available, add together all the mask values for the orders. The nil order resets all order options. See table below."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "overcharged",
    "prefix": "#overcharged",
    "body": "~#overcharged~",
    "description": "Grants the overcharged ability."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "owner",
    "prefix": "#owner",
    "body": "~#owner~",
    "description": " Changes the ownership of the active province. Nation nbr indicates the new owner. Nation numbers can be found in the Early Era Nations table and the three following tables."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "ownsmonrec",
    "prefix": "#ownsmonrec",
    "body": "~#ownsmonrec~",
    "description": "This monster can only be recruited if a unit of monster name type is owned by the recruiting player. Montag numbers can be used."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "path",
    "prefix": "#path",
    "body": "~#path~",
    "description": "The path of the spell OR site. \nSpell: First value should be 0 for the first required path and 1 for the second required path. The path nbr should be a number between -1 and 8. \nSite: The first value should be the path."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "pathboost",
    "prefix": "#pathboost",
    "body": "~#pathboost~",
    "description": "The mage who gains this event has the specified path level increased by 1, to a maximum of 5. The target of this event is set by the target requirements of the event."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "pathcost",
    "prefix": "#pathcost",
    "body": "~#pathcost~",
    "description": "The cost for a new path in design points when this monster is used as a god. Standard is a value between 10 (arch mage) and 80 (dragon). By using this command the monster will also be selectable as a god."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "pathlevel",
    "prefix": "#pathlevel",
    "body": "~#pathlevel~",
    "description": "Level required to cast this spell."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "patience",
    "prefix": "#patience",
    "body": "~#patience~",
    "description": "The patience value increases the chances of the assassin to catch his target unawares and without bodyguards. Each point of patience decreases the chance of bodyguards being present by 10%. If the bodyguard unit has the Bodyguard ability it reduces the effectiveness of the assassins Patience."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "patrolbonus",
    "prefix": "#patrolbonus",
    "body": "~#patrolbonus~",
    "description": "A value of ten will make this monster count as ten extra humans when it comes to patrolling. This value can also be negative for extra poor performance."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "pen",
    "prefix": "#pen",
    "body": "~#pen~",
    "description": "The item grants a penetration bonus that makes spells cast by the bearer harder to resist."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "pierce",
    "prefix": "#pierce",
    "body": "~#pierce~",
    "description": "The weapon does piercing damage. Monsters with Pierce Resistance only take half damage."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "pierceres",
    "prefix": "#pierceres",
    "body": "~#pierceres~",
    "description": "The monster takes half damage from piercing weapons."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "pillagebonus",
    "prefix": "#pillagebonus",
    "body": "~#pillagebonus~",
    "description": "A monster with this ability excels at pillaging. A barbarian has a pillage bonus of one which makes them count as one man extra when it comes to pillaging."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "plainshape",
    "prefix": "#plainshape",
    "body": "~#plainshape~",
    "description": "The monster changes to this shape when moving from a forest province to a non-forest province. If the monster also has a watershape the watershape takes precedence over the plainshape such as when moving from a forest to a water province."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "plant",
    "prefix": "#plant",
    "body": "~#plant~",
    "description": "The monster is a plant. Plants cannot be turned into zombies by Raise Dead or other methods."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "poison",
    "prefix": "#poison",
    "body": "~#poison~",
    "description": "One commander determined by target requirements is poisoned. The poison effect is armor negating but the damage will vary a bit anyway because of the usual +2d6 -2d6 calculations."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "poisonarmor",
    "prefix": "#poisonarmor",
    "body": "~#poisonarmor~",
    "description": "Anyone striking this monster with short weapons will be poisoned."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "poisoncloud",
    "prefix": "#poisoncloud",
    "body": "~#poisoncloud~",
    "description": "Monster is surrounded by a poison cloud. Standard size is 6."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "poisonres",
    "prefix": "#poisonres",
    "body": "~#poisonres~",
    "description": "The item grants a Poison Resistance bonus."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "poisonskin",
    "prefix": "#poisonskin",
    "body": "~#poisonskin~",
    "description": "The monster has a skin that exudes a paralyzing poison which does fatigue damage to anyone attacking it with length 0 weapons. The value is the damage rating of the poison."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "polygetmagic",
    "prefix": "#polygetmagic",
    "body": "~#polygetmagic~",
    "description": "1 means a unit polymorphed by this ritual will get the magic of the target creature."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "polyimmune",
    "prefix": "#polyimmune",
    "body": "~#polyimmune~",
    "description": "This monster cannot be polymorphed in combat."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "pooramphibian",
    "prefix": "#pooramphibian",
    "body": "~#pooramphibian~",
    "description": "This monster can travel under water but is hindered by it."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "poorleader",
    "prefix": "#poorleader",
    "body": "~#poorleader~",
    "description": "Leadership value 10. This is the standard value for mages. Commander cost +15. Commanded units have a morale modifier of -1 if in a single squad and an additional -1 for every additional squad."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "poormagicleader",
    "prefix": "#poormagicleader",
    "body": "~#poormagicleader~",
    "description": "This monster has an innate ability to command 10 magic beings. This is a very rare ability that only most magic beings possess."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "poorundeadleader",
    "prefix": "#poorundeadleader",
    "body": "~#poorundeadleader~",
    "description": "This monster has an innate ability to command 10 undead beings. This is a rare ability that usually only demons or undead beings possess."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "popkill",
    "prefix": "#popkill",
    "body": "~#popkill~",
    "description": "The monster kills [10 x amount] of population in the province it resides in every month. This applies to both commander and non-commander units with this attribute."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "poppergold",
    "prefix": "#poppergold",
    "body": "~#poppergold~",
    "description": "The amount of people required for one gold in taxes. Default is 100."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "poptype",
    "prefix": "#poptype",
    "body": "~#poptype~",
    "description": " Sets the population type of the active province. This determines which troops may be recruited in the province. Poptype numbers can be found in the large table on the following page. If poptype is set with a number higher than existing poptypes"
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "population",
    "prefix": "#population",
    "body": "~#population~",
    "description": " Sets the population number of the active province."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "prec",
    "prefix": "#prec",
    "body": "~#prec~",
    "description": "The item grants a precision bonus."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "precision",
    "prefix": "#precision",
    "body": "~#precision~",
    "description": "Set the precision for this spell."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "priestreanim",
    "prefix": "#priestreanim",
    "body": "~#priestreanim~",
    "description": "All priests of this nation are able to reanimate the dead as if the had the #reanimpriest attribute. They also gain the 15 points of undead leadership per holy magic level they would have received from that attribute."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "prophetshape",
    "prefix": "#prophetshape",
    "body": "~#prophetshape~",
    "description": "Unit will change into this monster when made a prophet. Monster nbr can be negative for montag usage."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "prot",
    "prefix": "#prot",
    "body": "~#prot~",
    "description": "The monsters natural protection. This value should be 0 for all humans 5 for a lizardman or about 18 for a huge and scaly dragon."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "provrange",
    "prefix": "#provrange",
    "body": "~#provrange~",
    "description": "Sets the range of a ritual in provinces."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "purgecalendar",
    "prefix": "#purgecalendar",
    "body": "~#purgecalendar~",
    "description": "Removes an event determined by attr from the global event calendar.  1 = the event that had been added after this event would be affected. 0 = current event -1 = the previous event. The event calendar contains the planned global events that will occur in the coming 7 months."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "purgedelayed",
    "prefix": "#purgedelayed",
    "body": "~#purgedelayed~",
    "description": "Removes an event from the delayed event list (same attr as previous command). Delayed events are events added by the #delay command."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "quadruped",
    "prefix": "#quadruped",
    "body": "~#quadruped~",
    "description": "Four legged beasts. Use this for Elephants lions wolves and similar."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "quickness",
    "prefix": "#quickness",
    "body": "~#quickness~",
    "description": "The item grants Quickness (double movement +2 attack & defense attacks twice as often)."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "raiseonkill",
    "prefix": "#raiseonkill",
    "body": "~#raiseonkill~",
    "description": "Monster has a chance in percent to raise the people it kills as soulless."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "raiseshape",
    "prefix": "#raiseshape",
    "body": "~#raiseshape~",
    "description": "Changes soulless to another kind of unit for the #raiseonkill or #dt_raise effects. Monster number can be negative for montag usage."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "randequip",
    "prefix": "#randequip",
    "body": "~#randequip~",
    "description": "How powerful magic equipment the commander will have. 0=none 3=extremely powerful stuff."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "randomequip",
    "prefix": "#randomequip",
    "body": "~#randomequip~",
    "description": " Gives random magic items to the active commander. Rich must be between 0 and 4. A value of 0 means small chance of getting a magic item and 4 means large chance of getting many powerful items."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "randomspell",
    "prefix": "#randomspell",
    "body": "~#randomspell~",
    "description": "The monster has a"
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "range",
    "prefix": "#range",
    "body": "~#range~",
    "description": "Sets the battlefield range for this spell. Add 5000 to this value to give longer range (5 extra per level) to more powerful casters."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "range0",
    "prefix": "#range0",
    "body": "~#range0~",
    "description": "This ranged weapon can also be used in melee."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "range050",
    "prefix": "#range050",
    "body": "~#range050~",
    "description": "This ranged weapon has a 50% chance of being used in melee."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "raredomsummon",
    "prefix": "#raredomsummon",
    "body": "~#raredomsummon~",
    "description": "There is a flat 8% chance of summoning one creature of this type when in a province with friendly dominion."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "rarity",
    "prefix": "#rarity",
    "body": "~#rarity~",
    "description": "Sets the rarity of the event. Normal events should use number 1, 2, -1, -2, -11 or -12. The other numbers are always events that are best used for creating global enchantment effects or special dominion effects."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "rcost",
    "prefix": "#rcost",
    "body": "~#rcost~",
    "description": "The cost in resources. The resource cost of weapons and armor will be added to this. Most human troops have a rcost of one. Heavy cavalry have a base rcost of 8 - 20 (to represent the barding of the mount) combined with #ressize 2 for human-sized armor and weapons."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "reanimator",
    "prefix": "#reanimator",
    "body": "~#reanimator~",
    "description": "The monster will reanimate this number of corpses automatically every turn."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "reanimpriest",
    "prefix": "#reanimpriest",
    "body": "~#reanimpriest~",
    "description": "A priest with this attribute will be able to raise undead."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "recallgod",
    "prefix": "#recallgod",
    "body": "~#recallgod~",
    "description": "The nation is better at Call God than other nations. This value is added to the priest level of all priests when they perform Call God."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "reclimit",
    "prefix": "#reclimit",
    "body": "~#reclimit~",
    "description": "The monster can only be recruited in limited numbers. You can only recruit"
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "recrate",
    "prefix": "#recrate",
    "body": "~#recrate~",
    "description": "The rate at which new troops are bought. A value of 100 means one per month."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "recuperation",
    "prefix": "#recuperation",
    "body": "~#recuperation~",
    "description": "The item grants the recuperation ability."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "reform",
    "prefix": "#reform",
    "body": "~#reform~",
    "description": "This is a combat ability. The monster has this chance of getting a new healthy body when killed."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "reformtime",
    "prefix": "#reformtime",
    "body": "~#reformtime~",
    "description": " Alters the time taken to reform the immortal\u00e2\u20ac\u2122s body. The standard time is 3 months, use #reformtime -2 if you want the immortal to reappear the next month."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "regeneration",
    "prefix": "#regeneration",
    "body": "~#regeneration~",
    "description": "The monster regenerates like a troll and heals damage every combat round. The percent value indicates how many percent of its total hit points are regenerated every turn. The standard value for regeneration is 10."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "reinvigoration",
    "prefix": "#reinvigoration",
    "body": "~#reinvigoration~",
    "description": "Reduces the fatigue by points amount every turn in battle. This value can be negative to gain extra fatigue in battle instead. Lower fatigue reduces the chance of the monster suffering critical hits while higher fatigue increases it."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "removesite",
    "prefix": "#removesite",
    "body": "~#removesite~",
    "description": "Removes a site from the province. Use -1 for site number to place [sitename] instead. [sitename] must be added to event message text in brackets. The site will be be removed regardless of being found or not."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_2monsters",
    "prefix": "#req_2monsters",
    "body": "~#req_2monsters~",
    "description": "This monster must be present in the province and there must be at least 2 of them."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_5monsters",
    "prefix": "#req_5monsters",
    "body": "~#req_5monsters~",
    "description": "This monster must be present in the province and there must be at least 5 of them."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_anycode",
    "prefix": "#req_anycode",
    "body": "~#req_anycode~",
    "description": "Event code must be present somewhere in the world. Can be used multiple times for an event. If the command is used multiple times, one of the codes being present is sufficient to fulfill the requirement."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_arenadone",
    "prefix": "#req_arenadone",
    "body": "~#req_arenadone~",
    "description": "The will require that an arena battle has taken place (been resolved) earlier this turn. If set to zero an arena battle must not have taken place."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_capital",
    "prefix": "#req_capital",
    "body": "~#req_capital~",
    "description": "Event can only happen in capital provinces. This includes conquered capital provinces.  1 = only in capitals 0 = never in capitals"
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_cave",
    "prefix": "#req_cave",
    "body": "~#req_cave~",
    "description": "Event requires the province to have cave terrain. 1 = must be cave, 0 = cannot be cave."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_chaos",
    "prefix": "#req_chaos",
    "body": "~#req_chaos~",
    "description": "Turmoil scale required."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_claimedthrone",
    "prefix": "#req_claimedthrone",
    "body": "~#req_claimedthrone~",
    "description": "The throne [sitename] must be claimed by someone. Combine with #req_site to target the throne province."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_coast",
    "prefix": "#req_coast",
    "body": "~#req_coast~",
    "description": "Event requires the province to have coastal terrain. 1 = must be coast, 0 = cannot be coast."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_code",
    "prefix": "#req_code",
    "body": "~#req_code~",
    "description": "Province must have this event code. Can be used multiple times for an event. If the command is used multiple times, one of the codes being present is sufficient to fulfill the requirement. Use #req_code 0 for events that generate event codes. Otherwise they may break ongoing event chains."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_cold",
    "prefix": "#req_cold",
    "body": "~#req_cold~",
    "description": "Cold scale required."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_commander",
    "prefix": "#req_commander",
    "body": "~#req_commander~",
    "description": "There must be a non-sneaking commander present for the event to happen. 1 = yes, 0 = no."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "req_deadmnr",
