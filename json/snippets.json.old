  {
    "\u00ef\u00bb\u00bfFriendly name": "constlevel",
    "prefix": "#constlevel",
    "body": "~#constlevel~",
    "description": "Level of construction required to forge this item. This level can be 02468 or 12 for items that cannot be forged. To disable an item set this value to 12."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "copyarmor",
    "prefix": "#copyarmor",
    "body": "~#copyarmor~",
    "description": "Copies all stats from an existing armor. This command will overwrite all stats for the selected armor so it should be used as the first command after the #newarmor command."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "copyitem",
    "prefix": "#copyitem",
    "body": "~#copyitem~",
    "description": "Copies all stats from an existing item to the current item."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "copyspell",
    "prefix": "#copyspell",
    "body": "~#copyspell~",
    "description": "Copies all stats including name from the specified spell to the current spell."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "copyspr",
    "prefix": "#copyspr",
    "body": "~#copyspr~",
    "description": "Copies the sprite of the specified magic item to the current magic item."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "copystats",
    "prefix": "#copystats",
    "body": "~#copystats~",
    "description": "Copies all weapons armors stats magic skills and special abilities from an existing monster. The existing monster can be a monster that has been created previously in the mod. Copystats will overwrite all previous commands for the selected monster so it should be used as the first command after the #newmonster command."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "copyweapon",
    "prefix": "#copyweapon",
    "body": "~#copyweapon~",
    "description": "Copies all stats from an existing weapon. This command will overwrite all stats for the selected weapon so it should be used as the first command after the #newweapon command."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "corpseeater",
    "prefix": "#corpseeater",
    "body": "~#corpseeater~",
    "description": "The monster will eat this amount of corpses per month if possible and gain HP for it equal to the value in #deadhp."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "corpselord",
    "prefix": "#corpselord",
    "body": "~#corpselord~",
    "description": "This monster will receive extra units when creating Corpse Men. This ability will not be visible when inspecting the monster."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "crippled",
    "prefix": "#crippled",
    "body": "~#crippled~",
    "description": "The item bearer becomes Crippled. The affliction cannot be healed until the item is removed."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "crossbreeder",
    "prefix": "#crossbreeder",
    "body": "~#crossbreeder~",
    "description": "The monster is skilled in crossbreeding and gets a bonus when using the Crossbreeding or Improved Crossbreeding ritual from the Blood magic school. The crossbreeder receives more units per casting and a higher chance of extraordinary results."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "curse",
    "prefix": "#curse",
    "body": "~#curse~",
    "description": "Every unit in the province has this chance of being cursed."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "cursed",
    "prefix": "#cursed",
    "body": "~#cursed~",
    "description": "The item is cursed and cannot be dropped."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "curseluckshield",
    "prefix": "#curseluckshield",
    "body": "~#curseluckshield~",
    "description": "Grants the Fateweaving ability. Anyone striking the unit will have their fate rewritten and suffer from misfortunes for the next few rounds of battle. The effect can be resisted with a successful Magic Resistance check. The penetration bonus makes the effect harder to resist."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "custommagic",
    "prefix": "#custommagic",
    "body": "~#custommagic~",
    "description": "Gives a chance for another magic skill to the active monster. The path mask must be a number from Table 18 or a sum of multiple masks from this table."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "damage",
    "prefix": "#damage",
    "body": "~#damage~",
    "description": "Set the damage for this spell. Add 1000 to dmg to make the damage be variable with the level of the caster. Damage means damage for damage spells but it can also mean many other things for different spells. E.g. summoning spells use this value to determine which monster is summoned."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "damagemon",
    "prefix": "#damagemon",
    "body": "~#damagemon~",
    "description": "Use this to set a spells damage to a monster if you dont know its number."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "damagerev",
    "prefix": "#damagerev",
    "body": "~#damagerev~",
    "description": "The monster has the Damage Reversal ability. Any creature that hits the monster must make a magic resistance check or suffer the damage itself instead of harming the target. Strength must be at least one and strength minus one is the penalty to the MR check. Use strength 1 for standard damage reversal with no penalty."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "darkpower",
    "prefix": "#darkpower",
    "body": "~#darkpower~",
    "description": "The monster will get stat increases in darkness. Only active in combat."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "darkvision",
    "prefix": "#darkvision",
    "body": "~#darkvision~",
    "description": "Gives monster darkvision lessening penalties for fighting under darkness. Use value 100 for perfect darkvision."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "deadhp",
    "prefix": "#deadhp",
    "body": "~#deadhp~",
    "description": "Number of HP gained per corpse eaten."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "deathattuned",
    "prefix": "#deathattuned",
    "body": "~#deathattuned~",
    "description": "Chance of getting a bonus level of magic when transformed into this being.  In place of [path] insert the name of the magic path in all lower case for the correct command (#fireattuned #airattuned etc)."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "deathbanish",
    "prefix": "#deathbanish",
