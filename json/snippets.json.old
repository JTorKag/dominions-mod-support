  {
    "\u00ef\u00bb\u00bfFriendly name": "clearnation",
    "prefix": "#clearnation",
    "body": "~#clearnation~",
    "description": "Clears away all special settings for the nation like ideal cold reanimating priests underwater nation starting sites heroes pretender god list local defense fort type etc. The list of recruitable units and commanders is not cleared with this command."
  },
  {
    "\u00ef\u00bb\u00bfFriendly name": "clearrec",
    "prefix": "#clearrec",
    "body": "~#clearrec~",
    "description": "Clears the recruitment list of all units and commanders."
  },
