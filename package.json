{
  "name": "dominions-mod-support",
  "displayName": "Dominions Mod Support",
  "description": "Syntax highlighting & Hover information on keywords and IDs.",
  "version": "1.0.0",
  "repository": "https://github.com/JTorKag/dominions-mod-support",
  "publisher": "jtorkag",
  "license":"LGPL-3.0-only",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages","Snippets"
  ],
  "main": "./hover.js",
  "contributes": {
    "languages": [{
      "id": "dominionsmod",
      "aliases": ["Dominions 5 Mod", "dominionsmod"],
      "extensions": [".dm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "dominionsmod",
      "scopeName": "source.dm",
      "path": "./syntaxes/dominionsmod.tmLanguage.json"
    }],
    "snippets": [{
        "language": "dominionsmod",
        "path": "./snippets.json"
    }]
  }
}
